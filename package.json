    "tsx": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plurigrid/gay-tofu"
  },
  "exports": {
    ".": {
      "import": "./gay-tofu.ts",
      "types": "./gay-tofu.ts"
    }
  }
